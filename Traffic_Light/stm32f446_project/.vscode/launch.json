    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug stm32f446_project",
            "request": "launch",
            "type": "cortex-debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}\\target\\thumbv7em-none-eabihf\\debug\\stm32f446_project",
            "svdFile": "${workspaceFolder}\\stm32f446.svd",
            "servertype": "openocd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f4x.cfg"
            ],
            "preLaunchTask": "Build binary",
            "preLaunchCommands": [
                "monitor reset halt",
                "monitor flash write_image erase D:/Workspace/STM/Projects/Mithunvoe/Traffic_Light/stm32f446_project/target/thumbv7em-none-eabihf/debug/stm32f446_project.bin 0x08000000",
                "monitor reset halt"
            ],
            "postLaunchCommands": [
                "continue"
            ],
            "runToEntryPoint": "main",
            "device": "STM32F446RE"
        }
    ]
}