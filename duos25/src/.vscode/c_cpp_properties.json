{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/kern/include",
                "${workspaceFolder}/kern/arch/include",
                "${workspaceFolder}/kern/arch/include/cm4",
                "${workspaceFolder}/kern/arch/stm32f446re/include",
                "${workspaceFolder}/kern/include/kern",
                "${workspaceFolder}/kern/dev/include",
                "${workspaceFolder}/userland/include",
                "${workspaceFolder}/kern/sys_config",
                "${workspaceFolder}/kern/include/sotom"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "cStandard": "gnu11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm",
            "compilerArgs": [
                "-mcpu=cortex-m4",
                "-mthumb",
                "-mfloat-abi=softfp",
                "-mfpu=fpv4-sp-d16"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/kern",
                    "${workspaceFolder}/userland"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}
